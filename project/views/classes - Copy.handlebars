
<div class="col-sm-3 col-md-2 sidebar" style="width:15%; float:left; border:5px solid gray;">
    <h1> BCIS Courses</h1>
    <ul class="nav nav-sidebar">
        {{#each courses}} 
        <li><a href="/courses/{{Class_ID}}">{{Class_ID}}</a></li>
        {{/each}}
    </ul>
</div>
<div style="display:inline-block; margin: 0 auto; border:5px solid gray; width: 70%; height: 700px;">
    {{#if course}}
     <p><strong>Class ID:</strong>{{course.Class_ID}}</p><br>
     <p><strong>Class Name:</strong>{{course.Class_Name}}</p><br>
     <p><strong>Class Description:</strong>{{course.Class_Description}}</p><br>
     <p><strong>Professor:</strong>{{course.Prof_Name}}</p><br>
    <div class='embed-responsive' style="height:100%">
        <embed src="/static/documents/Syllabus/{{course.SyllabusFilePath}}" width='100%' height='100%'>
    </div>
    {{/if}}
</div>
<div class="col-sm-3 col-md-2 sidebar" style="width:15%;float: right;border:5px solid gray;">
    <h1> BCIS Courses</h1>
    <ul class="nav nav-sidebar">
        {{#each courses}} 
        <li><a href="/courses/{{Class_ID}}">{{Class_ID}}</a></li>
        {{/each}}
    </ul>
</div>
