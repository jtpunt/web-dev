<nav class="navbar navbar-inverse navbar-fixed" id="left-sidebar-wrapper" role="navigation">
    <ul class="nav sidebar-nav">
        <li class="sidebar-brand">
            <a href="#">
               BCIS Courses
            </a>
        </li>
        {{#each courses}} 
        <li class="nav-item">
            <a id="{{Class_ID}}" class="classLinks" onclick='bindButtons(event, "{{Class_ID}}", "{{Class_Name}}", "{{Semester_Taken}}", "{{Prof_Name}}", "{{SyllabusFilePath}}", "{{FinalGrade}}");'>{{Class_ID}}</a>
        </li>
        {{/each}}
    </ul>
</nav>
<div id="maincontent">
    <div class="tab">
        <button id="btnClass" class="tablinks">Class Information</button>
        <button id="btnSyllabus" class="tablinks">View Syllabus</button>
        <button id="btnProject" class="tablinks">View Projects</button>
    </div>
    <div id ="classInfo" class="tabcontent">
        <p><strong>Class ID:</strong><span id="Class_ID"></span></p>
        <p><strong>Class Name: </strong><span id="Class_Name"></span></p>             
        <p><strong>Semester Taken: </strong><span id="Semester_Taken"></span></p>          
        <p><strong>Professor: </strong><span id="Prof_Name"></span></p>             
        <p><strong>Syllabus Filename: </strong><span id="SyllabusFilePath"></span></p>             
        <p><strong>Grade Received: </strong><span id="FinalGrade"></span></p>
    </div>
    <div id="syllabusInfo" class="tabcontent">
        <iframe src="" style="width:100%;height:100%;"></iframe>
    </div>
    <div id="projectInfo" class="tabcontent">
        Projects: 
        <select id="project"> 
        {{#each projects}}
            <option class="projects" value="{{Class_ID}}">{{ProjectFile}}</option>
        {{/each}}
        </select><br>
        <iframe id="projectInfoPDF" src="" style="width:100%;height:100%;"></iframe>
    </div>
</div>
<div class="col-sm-3 col-md-2 sidebar " id="right-sidebar-wrapper">
    <h1> BCIS Courses</h1>
    <ul class="nav nav-sidebar">
        {{#each courses}} 
        <li><a href="/courses/{{Class_ID}}">{{Class_ID}}</a></li>
        {{/each}}
    </ul>
</div>
